{
    "name": "swooletw/hyperf-dispatcher",
    "type": "library",
    "description": "The dispatcher package for Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "dispatcher",
        "swoole"
    ],
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert.cht@pm.me",
            "homepage": "https://albert-chen.com"
        }
    ],
    "support": {
        "issues": "https://github.com/swooletw/hyperf-packages/issues",
        "source": "https://github.com/swooletw/hyperf-packages"
    },
    "autoload": {
        "psr-4": {
            "SwooleTW\\Hyperf\\Dispatcher\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "hyperf/http-server": "~3.1.0",
        "hyperf/context": "~3.1.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": "SwooleTW\\Hyperf\\Dispatcher\\ConfigProvider"
        },
        "branch-alias": {
            "dev-main": "3.1-dev"
        }
    }
}
