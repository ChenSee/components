{
    "name": "swooletw/hyperf-database",
    "description": "The database package for Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "database",
        "eloquent",
        "swoole",
        "larvel"
    ],
    "support": {
        "issues": "https://github.com/swooletw/hyperf-packages/issues",
        "source": "https://github.com/swooletw/hyperf-packages"
    },
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert.cht@pm.me",
            "homepage": "https://albert-chen.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "hyperf/database": "^3.1",
        "hyperf/db-connection": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "SwooleTW\\Hyperf\\Database\\": "src/"
        }
    },
    "extra": {
        "hyperf": {
            "config": "SwooleTW\\Hyperf\\Database\\ConfigProvider"
        },
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}