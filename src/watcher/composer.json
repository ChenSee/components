{
    "name": "swooletw/hyperf-watcher",
    "description": "The Watcher package for Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "watcher",
        "swoole"
    ],
    "support": {
        "issues": "https://github.com/swooletw/hyperf-packages/issues",
        "source": "https://github.com/swooletw/hyperf-packages"
    },
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert.cht@pm.me",
            "homepage": "https://albert-chen.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "hyperf/watcher": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "SwooleTW\\Hyperf\\Watcher\\": "src/"
        }
    },
    "extra": {
        "hyperf": {
            "config": "SwooleTW\\Hyperf\\Watcher\\ConfigProvider"
        },
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
