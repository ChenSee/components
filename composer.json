{
    "name": "laravel-hyperf/components",
    "type": "library",
    "description": "The components for Laravel Hyperf framework.",
    "license": "MIT",
    "keywords": [
        "php",
        "laravel",
        "hyperf",
        "laravel-hyperf",
        "components",
        "swoole"
    ],
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@laravel-hyperf.com"
        }
    ],
    "support": {
        "issues": "https://github.com/laravel-hyperf/components/issues",
        "source": "https://github.com/laravel-hyperf/components"
    },
    "autoload": {
        "psr-4": {
            "Illuminate\\Events\\": "src/event/illuminate/",
            "LaravelHyperf\\": "src/core/src/",
            "LaravelHyperf\\Auth\\": "src/auth/src/",
            "LaravelHyperf\\Broadcasting\\": "src/broadcasting/src/",
            "LaravelHyperf\\Bus\\": "src/bus/src/",
            "LaravelHyperf\\Cache\\": "src/cache/src/",
            "LaravelHyperf\\Config\\": "src/config/src/",
            "LaravelHyperf\\Container\\": "src/container/src/",
            "LaravelHyperf\\Cookie\\": "src/cookie/src/",
            "LaravelHyperf\\Coroutine\\": "src/coroutine/src/",
            "LaravelHyperf\\Devtool\\": "src/devtool/src/",
            "LaravelHyperf\\Dispatcher\\": "src/dispatcher/src/",
            "LaravelHyperf\\Encryption\\": "src/encryption/src/",
            "LaravelHyperf\\Event\\": "src/event/src/",
            "LaravelHyperf\\Filesystem\\": "src/filesystem/src/",
            "LaravelHyperf\\Foundation\\": "src/foundation/src/",
            "LaravelHyperf\\Hashing\\": "src/hashing/src/",
            "LaravelHyperf\\Http\\": "src/http/src/",
            "LaravelHyperf\\HttpClient\\": "src/http-client/src/",
            "LaravelHyperf\\JWT\\": "src/jwt/src/",
            "LaravelHyperf\\Log\\": "src/log/src/",
            "LaravelHyperf\\Mail\\": "src/mail/src/",
            "LaravelHyperf\\Notifications\\": "src/notifications/src/",
            "LaravelHyperf\\ObjectPool\\": "src/object-pool/src/",
            "LaravelHyperf\\Prompts\\": "src/prompts/src/",
            "LaravelHyperf\\Queue\\": "src/queue/src/",
            "LaravelHyperf\\Router\\": "src/router/src/",
            "LaravelHyperf\\Session\\": "src/session/src/",
            "LaravelHyperf\\Support\\": "src/support/src/",
            "LaravelHyperf\\Telescope\\": "src/telescope/src/"
        },
        "files": [
            "src/auth/src/Functions.php",
            "src/bus/src/Functions.php",
            "src/cache/src/Functions.php",
            "src/config/src/Functions.php",
            "src/coroutine/src/Functions.php",
            "src/event/src/Functions.php",
            "src/filesystem/src/Functions.php",
            "src/foundation/src/helpers.php",
            "src/prompts/src/helpers.php",
            "src/router/src/Functions.php",
            "src/session/src/Functions.php",
            "src/support/src/Functions.php",
            "src/support/src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelHyperf\\Tests\\": "tests/"
        },
        "exclude-from-classmap": [
            "tests/Foundation/fixtures/hyperf/database/migrations/"
        ]
    },
    "require": {
        "php": ">=8.2",
        "ext-hash": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "composer-runtime-api": "^2.2",
        "friendsofhyperf/command-signals": "~3.1.0",
        "friendsofhyperf/pretty-console": "~3.1.0",
        "guzzlehttp/guzzle": "^7.8.2",
        "guzzlehttp/uri-template": "^1.0",
        "hyperf/cache": "~3.1.0",
        "hyperf/command": "~3.1.0",
        "hyperf/config": "~3.1.0",
        "hyperf/crontab": "~3.1.0",
        "hyperf/database-sqlite": "~3.1.0",
        "hyperf/db-connection": "~3.1.0",
        "hyperf/dispatcher": "~3.1.0",
        "hyperf/engine": "^2.10",
        "hyperf/framework": "~3.1.0",
        "hyperf/http-server": "~3.1.0",
        "hyperf/memory": "~3.1.0",
        "hyperf/process": "~3.1.0",
        "hyperf/resource": "~3.1.0",
        "hyperf/signal": "~3.1.0",
        "hyperf/translation": "~3.1.0",
        "hyperf/validation": "~3.1.0",
        "laravel/serializable-closure": "^1.3",
        "lcobucci/jwt": "^5.0",
        "league/commonmark": "^2.2",
        "monolog/monolog": "^3.1",
        "nesbot/carbon": "^2.72.6",
        "nunomaduro/termwind": "^2.0",
        "psr/log": "^1.0|^2.0|^3.0",
        "ramsey/uuid": "^4.7",
        "symfony/error-handler": "^6.3",
        "symfony/mailer": "^6.2",
        "tijsverkoyen/css-to-inline-styles": "^2.2.5"
    },
    "replace": {
        "laravel-hyperf/auth": "self.version",
        "laravel-hyperf/broadcasting": "self.version",
        "laravel-hyperf/bus": "self.version",
        "laravel-hyperf/cache": "self.version",
        "laravel-hyperf/config": "self.version",
        "laravel-hyperf/container": "self.version",
        "laravel-hyperf/cookie": "self.version",
        "laravel-hyperf/core": "self.version",
        "laravel-hyperf/coroutine": "self.version",
        "laravel-hyperf/devtool": "self.version",
        "laravel-hyperf/dispatcher": "self.version",
        "laravel-hyperf/encryption": "self.version",
        "laravel-hyperf/event": "self.version",
        "laravel-hyperf/filesystem": "self.version",
        "laravel-hyperf/foundation": "self.version",
        "laravel-hyperf/hashing": "self.version",
        "laravel-hyperf/http": "self.version",
        "laravel-hyperf/http-client": "self.version",
        "laravel-hyperf/jwt": "self.version",
        "laravel-hyperf/log": "self.version",
        "laravel-hyperf/mail": "self.version",
        "laravel-hyperf/notifications": "self.version",
        "laravel-hyperf/object-pool": "self.version",
        "laravel-hyperf/prompts": "self.version",
        "laravel-hyperf/queue": "self.version",
        "laravel-hyperf/router": "self.version",
        "laravel-hyperf/session": "self.version",
        "laravel-hyperf/support": "self.version",
        "laravel-hyperf/telescope": "self.version"
    },
    "suggest": {
        "hyperf/redis": "Required to use redis driver. (^3.1).",
        "laravel-hyperf/session": "Required to use session guard. (^3.1).",
        "friendsofhyperf/tinker": "Required to use the tinker console command (^3.1).",
        "league/flysystem-read-only": "Required to use read-only disks (^3.3)",
        "aws/aws-sdk-php": "Required to use the SES mail driver (^3.235.5).",
        "symfony/http-client": "Required to use the Symfony API mail transports (^6.2).",
        "symfony/mailgun-mailer": "Required to enable support for the Mailgun mail transport (^6.2).",
        "symfony/postmark-mailer": "Required to enable support for the Postmark mail transport (^6.2).",
        "ably/ably-php": "Required to use the Ably broadcast driver (^1.0).",
        "pusher/pusher-php-server": "Required to use the Pusher broadcast driver (^6.0|^7.0)."
    },
    "require-dev": {
        "ably/ably-php": "^1.0",
        "fakerphp/faker": "^2.0",
        "filp/whoops": "^2.15",
        "friendsofphp/php-cs-fixer": "^3.57.2",
        "hyperf/devtool": "~3.1.0",
        "hyperf/redis": "~3.1.0",
        "hyperf/testing": "~3.1.0",
        "hyperf/view-engine": "~3.1.0",
        "league/flysystem": "^3.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/flysystem-google-cloud-storage": "^3.0",
        "league/flysystem-path-prefixing": "^3.3",
        "league/flysystem-read-only": "^3.3",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^8.5",
        "pda/pheanstalk": "v5.0.9",
        "phpstan/phpstan": "^1.11.5",
        "phpunit/phpunit": "^10.0.7",
        "pusher/pusher-php-server": "^7.2",
        "swoole/ide-helper": "~5.1.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "hyperf": {
            "config": [
                "LaravelHyperf\\ConfigProvider",
                "LaravelHyperf\\Auth\\ConfigProvider",
                "LaravelHyperf\\Broadcasting\\ConfigProvider",
                "LaravelHyperf\\Bus\\ConfigProvider",
                "LaravelHyperf\\Cache\\ConfigProvider",
                "LaravelHyperf\\Cookie\\ConfigProvider",
                "LaravelHyperf\\Config\\ConfigProvider",
                "LaravelHyperf\\Devtool\\ConfigProvider",
                "LaravelHyperf\\Dispatcher\\ConfigProvider",
                "LaravelHyperf\\Encryption\\ConfigProvider",
                "LaravelHyperf\\Event\\ConfigProvider",
                "LaravelHyperf\\Filesystem\\ConfigProvider",
                "LaravelHyperf\\Foundation\\ConfigProvider",
                "LaravelHyperf\\Hashing\\ConfigProvider",
                "LaravelHyperf\\Http\\ConfigProvider",
                "LaravelHyperf\\JWT\\ConfigProvider",
                "LaravelHyperf\\Log\\ConfigProvider",
                "LaravelHyperf\\Mail\\ConfigProvider",
                "LaravelHyperf\\Notifications\\ConfigProvider",
                "LaravelHyperf\\Queue\\ConfigProvider",
                "LaravelHyperf\\Router\\ConfigProvider",
                "LaravelHyperf\\Session\\ConfigProvider",
                "LaravelHyperf\\Telescope\\ConfigProvider"
            ]
        },
        "laravel-hyperf": {
            "providers": [
                "LaravelHyperf\\Notifications\\NotificationServiceProvider",
                "LaravelHyperf\\Telescope\\TelescopeServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "0.1-dev"
        }
    },
    "minimum-stability": "dev"
}
